
page {
  background-color: white;
}

.edit-btn {
  position: fixed;
  bottom: 30px;
  right: 25px;
  width: 55px;
  height: 55px;
  border-radius: 50%;
  display: inline-flex;
  justify-content: center;
  align-items: center;
  box-shadow: 2px 1px 1.5px 1.5px #f2f2f2;
  background-color: white;
}

.edit-btn image {
  width: 50rpx;
  height: 50rpx;
}

.allImg {
  display: inline-flex;
  flex-direction: row;
}

.allImg image {
  margin: 5px 5px;
}

.cu-bar.btn-group {
  position: fixed;
  bottom: 0px;
  width: 100%;
}
