page {
  width: 100%;
  height: 100%;
  /* overflow: hidden; */
  background-color: #f2f2f2;
}

.background-image {
  width: 100%;
  height: 100%;
  background-size: cover;
}

.header {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  padding-left: 20rpx;
  padding-right: 20rpx;
  padding-top: 20rpx;
  align-items: center;
}

.header image {
  width: 40px;
  height: 40px;
  border-radius: 50%;
}

.text {
  display: flex;
  flex-direction: column;
  font-weight: 600;
}

.text text {
  color: white;
}

.text-1 {
  font-size: 14px;
}

.text-2 {
  font-size: 18px;
  margin-right: 5rpx;
  color: white;
}

.day {
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
}

.nickName {
  padding-left: 20rpx;
  padding-right: 20rpx;
  padding-top: 5rpx;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
}

.nickName text {
  color: white;
  font-size: 13px;
  margin-top: 5rpx;
}

.weather {
  padding-left: 30rpx;
  padding-right: 20rpx;
  padding-top: 10rpx;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
}

.weather-item image {
  width: 30rpx;
  height: 30rpx;
}

.weather-item {
  display: flex;
  flex-direction: row;
  align-items: center;
}

.weather-item text {
  font-size: 20rpx;
  color: white;
  margin-left: 5rpx;
}

.bar {
  display: flex;
  position: relative;
  align-items: center;
  height: 100rpx;
  justify-content: center;
  color: white;
}

.fun {
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
}

.utils {
  display: flex;
  white-space: nowrap;
  margin-top: 30rpx;
  background-color: rgba(255, 255, 255, 0.9);
  border-radius: 5px;
  position: fixed;
  bottom: 20px;
  width: 95%;
  overflow: hidden;
}

.item {
  display: inline-block;
  margin-right: 40rpx;
  padding: 20rpx;
}

.item-item {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}

.item-item image {
  width: 70rpx;
  height: 70rpx;
  margin-bottom: 20rpx;
}

.utils .item:last-child {
  margin-right: 0rpx;
}

