page {
  background-color: white;
}

.new {
  display: flex;
  flex-direction: row;
  justify-content: center;
  width: 100%;
  margin-top: 20px;
  margin-bottom: 20px;
}

.new view {
  display: flex;
  flex-direction: row;
  justify-content: space-around;
  background: rgb(68, 68, 68);
  border-radius: 5px;
}

.new view text {
  padding: 20rpx 10rpx 20rpx 10rpx;
  font-size: 14px;
}

.wux-segmented-control {
  position: absolute;
  text-align: center;
  display: flex;
  flex-direction: row;
  justify-content: center;
  left: 0;
  right: 0;
  bottom: 11rpx;
  margin: auto;
  height: 60rpx;
  font-size: 36rpx;
  line-height: 60rpx;
}

.item {
  display: flex;
  flex-direction: row;
  padding: 30rpx 20rpx 30rpx 20rpx;
  border-bottom: 1px solid #ccc;
}

.avatar {
  margin-right: 20rpx;
}

.avatar image {
  width: 70rpx;
  height: 70rpx;
  border-radius: 50%;
}

.item-item {
  display: flex;
  flex-direction: column;
  width: 90%;
}

.nickName {
  font-size: 16px;
  color: rgb(122, 122, 122);
  font-weight: 600;
  margin-bottom: 20rpx;
}

.calendar {
  font-weight: 600;
  display: flex;
  flex-direction: column;
}

.calendar-item {
  display: flex;
  flex-direction: row;
  flex-wrap: nowrap;
  margin: 20rpx 20rpx 10rpx 0rpx;
}

.day-day {
  display: flex;
  flex-direction: row;
  flex-wrap: nowrap;
}

.calendar-item-1 {
  width: 80%;
  padding: 10rpx;
  border: 1px solid #ccc;
  border-right: none;
  border-top-left-radius: 5rpx;
  border-bottom-left-radius: 5rpx;
  font-weight: 400;
}

.calendar-item-2 {
  width: 20%;
  border-top-right-radius: 5rpx;
  border-bottom-right-radius: 5rpx;
  border-left: none;
}

.diff {
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  color: white;
  width: 70%;
  padding: 10rpx;
}

.static {
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  color: white;
  width: 30%;
  border-top-right-radius: 5rpx;
  border-bottom-right-radius: 5rpx;
}

.days-color {
  background-color: rgb(247, 163, 5);
  border-top: 1px solid rgb(247, 163, 5);
  border-bottom: 1px solid rgb(247, 163, 5);
}

.day-color {
  background-color: rgb(236, 138, 3);
  border: 1px solid rgb(236, 138, 3);
  border-left: none;
}

.static-remind-day-color {
  background-color: #f199b1;
}

.static-remind-days-color {
  background-color: rgb(241, 171, 190);
}

.remind-days-color {
  background-color: rgb(95, 166, 254);
  border-top: 1px solid rgb(95, 166, 254);
  border-bottom: 1px solid rgb(95, 166, 254);
}

.remind-day-color {
  background-color: rgb(70, 158, 248);
  border: 1px solid rgb(70, 158, 248);
  border-left: none;
}

.date {
  display: flex;
  flex-direction: row;
  margin-top: 10rpx;
  justify-content: space-between;
  padding-right: 20rpx;
  align-items: center;
}

.date text {
  color: rgb(122, 122, 122);
  font-size: 12px;
}

.diary {
  display: flex;
  flex-direction: column;
  font-weight: 600;
}

.diary-detail {
  background-color: rgb(243, 243, 245);
  padding: 20rpx 15rpx 20rpx 15rpx;
  margin-right: 20rpx;
  margin-top: 20rpx;
  font-weight: 400;
  text-overflow: ellipsis;
  word-wrap: break-word;
}

.photo {
  width: 100%;
  display: flex;
  flex-direction: column;
}

.photo-item {
  width: 480rpx;
  display: inline-flex;
  flex-direction: row;
  flex-wrap: wrap;
}

.photo image {
  width: 150rpx;
  height: 150rpx;
  margin-right: 10rpx;
  margin-bottom: 10rpx;
}

.comments {
  background-color: rgb(243, 243, 245);
  margin-top: 20rpx;
  display: inline-flex;
  flex-direction: column;
  justify-content: center;
  margin-right: 20rpx;
}

.comments text {
  padding-top: 5rpx;
  padding-bottom: 5rpx;
}

.nickName1 {
  color: rgb(82, 110, 134);
  margin: 10rpx;
}

.nickName1 text {
  margin-left: 10rpx;
  color: black;
  font-weight: 400;
}

.comments:last-child {
  padding-bottom: 0rpx;
}

.edit-btn {
  position: fixed;
  bottom: 30px;
  right: 25px;
  width: 55px;
  height: 55px;
  border-radius: 50%;
  display: inline-flex;
  justify-content: center;
  align-items: center;
  box-shadow: 2px 1px 1.5px 1.5px #c5c5c5;
  background-color: white;
}

.edit-btn image {
  width: 50rpx;
  height: 50rpx;
}

scroll-view {
  text-align: center;
}

scroll-view .tab {
  margin-left: 20rpx;
  margin-right: 20rpx;
}

.tab {
  display: inline-flex;
  flex-direction: row;
  justify-content: space-around;
  padding-top: 15rpx;
  padding-bottom: 15rpx;
  font-size: 15px;
}

.line {
  width: 100%;
  height: 1rpx;
  background-color: #f2f2f2;
}

.tabcolor {
  color: #f597b1;
  border-bottom: 2px solid #f597b1;
}

.dataMany {
  padding: 6px;
  border-bottom: 1px solid #f2f2f2;
  width: 100%;
}

.brand{
  font-size: 16px;
  font-weight: 600;
  margin-bottom: 10rpx;
  margin-top: 10rpx;
}

.content{
  font-size: 13px;
  text-overflow: ellipsis;
  word-wrap: break-word;
  margin-bottom: 10rpx;
}

.advertisement{
  padding: 6px;
  width: 100%;
  display: flex;
  flex-direction: column;
}

.advertisement image{
  width: 100%;
  height: 300rpx;
}

.title {
  font-size: 18px;
  margin-top: 10px;
  margin-bottom: 10px;
  margin-left: 3px;
}

.content {
  margin-bottom: 10px;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  margin-left: 3px;
}

.img {
  display: inline-flex;
  flex-direction: row;
  margin: 3px;
}

.msg {
  display: flex;
  flex-direction: row;
  align-items: center;
  padding-top: 15rpx;
}

.msg-avatar image {
  width: 20px;
  height: 20px;
  border-radius: 50%;
}

.msg view {
  margin-right: 15rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
}

.ic {
  margin-left: 5rpx;
}

.uploadImgCount {
  position: absolute;
  bottom: 0px;
  right: 0;
  color: white;
  background-color: rgba(8, 8, 8, 0.5);
  padding: 10rpx;
  font-size: 12px;
}

.showMore {
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  padding: 30rpx;
  background-color: white;
}

.loading {
  width: 20px;
  height: 20px;
  display: inline-block;
  vertical-align: middle;
  animation: loading 1s steps(12, end) infinite;
  background: transparent url(data:image/svg+xml;base64,) no-repeat;
  background-size: 100%;
}

@keyframes loading {
  0% {
    transform: rotate3d(0, 0, 1, 0deg);
  }

  100% {
    transform: rotate3d(0, 0, 1, 360deg);
  }
}

.sex1 {
  color: blue;
  font-size: 13px;
  margin-left: 5rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
}

.sex2 {
  color: pink;
  font-size: 13px;
  margin-left: 5rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
}
